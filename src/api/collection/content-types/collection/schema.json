{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "collaborators": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "public",
        "shared_posts",
        "unlisted"
      ]
    },
    "saved_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::saved-post.saved-post",
      "mappedBy": "collection"
    }
  }
}
